{
  "name": "flowbite-react-icons",
  "version": "0.0.1",
  "description": "Official Flowbite Icons library for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist",
    "typecheck": "tsc",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/themesberg/flowbite-react-icons.git"
  },
  "keywords": [
    "flowbite",
    "react",
    "icons",
    "svg"
  ],
  "author": {
    "name": "Sutu Sebastian",
    "email": "sebastian.sutu@stainless-code.com",
    "url": "https://github.com/SutuSebastian"
  },
  "license": "MIT",
  "bugs": "https://github.com/themesberg/flowbite-react-icons/issues",
  "homepage": "https://github.com/themesberg/flowbite-react-icons#readme",
  "devDependencies": {
    "@types/react": "^18.2.38",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
  }
